{
    "name": "IP GeoLinker",
    "description": "Discover IP addresses mentioned on any webpage with instant tooltips providing valuable information.",
    "version": "1.0.1",
    "manifest_version": 3,
    "permissions": [
        "scripting", 
        "storage",
        "activeTab",
        "tabs"
        

    ],
    "background": {
        "service_worker": "./bg.js"
        
    },
    "host_permissions": [
        "http://*/",
        "https://*/"
      ],
    "optional_host_permissions": [
        "*://*/*"
    ],
  

    "icons": {
        "32": "media/icon32.ico",
        "48": "media/icon48.ico",
        "64": "media/icon64.ico",
        "128": "media/icon128.ico",
        "512": "media/icon512.ico"
      }




}